{
  "name": "planning-euchre",
  "version": "0.0.1",
  "description": "Generic scrum playing card planning app",
  "main": "client/src/entry.js",
  "keywords": [],
  "scripts": {
    "postinstall": "bower cache clean && bower install",
    "clean": "rimraf static/dist",
    "build": "yarn clean && webpack --config ./webpack.config.js --progress --profile --colors",
    "build:prod": "NODE_ENV=production yarn build",
    "watch": "yarn clean && webpack-dev-server --content-base static/ --hot --inline --config webpack.config.js",
    "serve": "yarn build && serve static",
    "start": "yarn watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ebenpack/planning-euchre.git"
  },
  "author": "ebenpack",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ebenpack/planning-euchre/issues"
  },
  "dependencies": {
    "bower": "^1.7.9",
    "create-react-class": "^15.6.0",
    "preact": "^8.1.0",
    "preact-compat": "^3.17.0",
    "purescript": "^0.11.4",
    "purescript-psa": "^0.5.1",
    "purs-loader": "^3.1.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "rimraf": "^2.5.2",
    "serve": "^5.1.5",
    "webpack": "^4.10.2",
    "webpack-cli": "^2.1.5",
    "webpack-dev-server": "^3.1.4",
    "webpack-node-externals": "^1.5.4",
    "xhr2": "^0.1.3"
  },
  "devDependencies": {
    "css-loader": "^0.28.11",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.0",
    "style-loader": "^0.21.0",
    "write-file-webpack-plugin": "^4.3.2"
  }
}
